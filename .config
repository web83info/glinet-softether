# SE_HUB5_VLANTAG=
# SE_HUB5_CASCADE_SERVERADDRESS=
# SE_HUB5_CASCADE_HUBNAME=
# SE_HUB5_CASCADE_USERNAME=
# SE_HUB5_CASCADE_PASSWORD=

# 04.スイッチ

# 05.ブリッジ

# 06.VLAN
# VLAN_LAN
# VLAN名
VLAN_LAN_NAME=VLAN_LAN

# VLAN ID
VLAN_LAN_VID=1

# VLANに紐付けるポートまたはイーサネット
# 	wan: 本体にWANまたは地球のアイコンが表示されているポート
#	lan1: wanの隣のポート
#	lan2: lan1の隣のポート
#	lan3: lan2の隣のポート
#	lan4: lan3の隣のポート
VLAN_LAN_PORTS="lan1"

# VLAN_WAN
VLAN_WAN_NAME=VLAN_WAN

# VLAN ID
VLAN_WAN_VID=2

# VLANに紐付けるポートまたはイーサネット
VLAN_WAN_PORTS="wan"

# VLAN
# 	VLANn_XXX
# 	n={1...10}

# VLAN 1
# VLAN名
VLAN1_NAME=VLAN_YOURNAME

# VLAN ID
VLAN1_VID=YOURPORT

# VLANに紐付けるポートまたはイーサネット
VLAN1_PORTS="lan1 lan2:t"

# 接続する仮想ハブの番号
# 	SE_HUBX_ の X の部分の数字を指定
VLAN1_HUB_TO=1

# VLAN 2
# VLAN2_NAME=
# VLAN2_VID=
# VLAN2_PORTS=
# VLAN2_HUB_TO=

# VLAN 3
