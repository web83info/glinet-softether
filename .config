# 09.WiFi
# 2.4GHz帯
# 無線使用
WIRELESS24_ENABLE=1

# チャンネル設定
WIRELESS24_CHANNEL=auto

# チャンネル設定値のリスト
WIRELESS24_CHANNELS="1 6 11"

# 国
WIRELESS24_COUNTRY=JP

# 5GHz帯
# 無線使用
WIRELESS5_ENABLE=1

# チャンネル設定
WIRELESS5_CHANNEL=auto

# チャンネル設定値のリスト
WIRELESS5_CHANNELS="36 40 44 48 52 56 60 64"

# 国
WIRELESS5_COUNTRY=JP

# 各WiFi
#   Slate:  16まで
#   Mango:   4まで
#   Shadow:  8まで

# WiFi1
# WiFi名
WIRELESS_WIFI1_NAME=SELF

# 周波数
WIRELESS_WIFI1_RADIO=5

# SSID
WIRELESS_WIFI1_SSID=SSID_FOR_ROUTER_LAN

# 暗号化方式
WIRELESS_WIFI1_ENCRYPTION=sae-mixed

# 暗号化キー
WIRELESS_WIFI1_KEY=your_ssid_key

# 接続するインターフェース
WIRELESS_WIFI1_INTERFACE=admin

