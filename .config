# VLAN_WAN
VLAN_WAN_NAME=VLAN_WAN
VLAN_WAN_VID=2
VLAN_WAN_PORTS="0t 1"

# VLAN (1...10)
# VLAN 1
VLAN1_NAME=VLAN_YOURNAME
VLAN1_VID=YOURPORT
VLAN1_PORTS="0t 3t"
VLAN1_IS_TAGGED=0

# 接続する仮想ハブの番号（SE_HUBX_ のXの部分の数字を指定）
